<!--pages/timeline/timeline.wxml-->
<view class='container'>
    <view class='line-lists'>
        <view class='line-list' wx:for='{{timelines}}'>
            <view class='time'>
                <view class='year-time'>2018</view>
                <view class='day-time'>01/19</view>
            </view>
            <view class='line'>
                <view class='love lw-love3'></view>
            </view>
            <view class='thing' style='background-image:url({{item.picurl}})'>
                <view class='mask'>
                    <view class='tit'>{{item.tit}}</view>
                    <view class='content'>{{item.description}}</view>
                </view>
            </view>
        </view>
