/* pages/photos/photos.wxss */

.container {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
}

.album {
    width: 300rpx;
    height: 300rpx;
    display: flex;
    flex-direction: column;
    text-align: center;
}

.album image {
    width: 100%;
    /* height: 100%; */
    border-radius: 5px;
}
.album text{
    font-size: 30rpx;
    margin-top: 15rpx;
}
